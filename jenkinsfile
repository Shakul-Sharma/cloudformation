pipeline {
    agent any
    parameters {
  string 'api_type'
  choice choices: ['dev '], name: 'environment'
}
    stages {
        stage('Create Stack') {
            steps {
            sh "aws cloudformation create-stack --stack-name s3bucket1 --template-body file://s3cft.json --region 'us-east-1'"
              }
             }
            }
            }
