pipeline {
    agent any
    parameters {
  choice choices: ['qa' , 'dev' , 'stg' , 'prod'], name: 'enviroment '
  string 'repo_tag'
  
}

    stages {
        stage('Create Stack') {
            steps {
            sh "aws cloudformation create-stack --stack-name s3bucket1 --template-body file://s3cft.json --region 'us-east-1'"
              }
             }
            }
            }
